{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "baseUrl": "src",
        "outDir": "dist"
    },
    "include": ["src/**/*"],
    "exclude": ["src/interfaces/**/*"]
}